int i;
	strcpy(first,word[0]);
    strcpy(last,word[0]);
	for (i=0;i<size-1;i++){
	    if (strcmp(word[i],word[i+1])>0){
	        if (strcmp(last,word[i])<0) strcpy(last,word[i]);
	        if (strcmp(first,word[i+1])>0) strcpy(first,word[i+1]);}
        else {if (strcmp(first,word[i])>0) strcpy(first,word[i]);
            if (strcmp(last,word[i+1])<0) strcpy(last,word[i+i]);}}
