   char j[] = "nine ";
   char k[] = "Input exceeds 99";
   if (num <100){
       switch (num%10){
           case 0: strcpy(str,a);
           break;
           case 1: strcpy(str,b);
           break;
           case 2: strcpy(str,c);
           break;
           case 3: strcpy(str,d);
           break;
           case 4: strcpy(str,e);
           break;
           case 5: strcpy(str,f);
           break;
           case 6: strcpy(str,g);
           break;
           case 7: strcpy(str,h);
           break;
           case 8: strcpy(str,i);
           break;
           case 9: strcpy(str,j);
           break;}
       switch (num/10){
           case 1: strcat(str,b);
           break;
           case 2: strcat(str,c);
           break;
           case 3: strcat(str,d);
           break;
           case 4: strcat(str,e);
           break;
           case 5: strcat(str,f);
           break;
           case 6: strcat(str,g);
           break;
           case 7: strcat(str,h);
           break;
           case 8: strcat(str,i);
           break;
           case 9: strcat(str,j);
           break;}
   }
   else strcpy(str,k);
