int i;
   float j=1.00;
   if (p>=0){
       for (i=0;i<p;i++){
       j *=num;
   }}
else{
    for (i=0;i<-p;i++){
        j /= num;
    }
}
return j;

//pointer
int i;
   *result=1;
   if (p>=0){
       for (i=0;i<p;i++){
       *result *=num;
   }}
else{
    for (i=0;i<-p;i++){
        *result /=num;
    }
}
