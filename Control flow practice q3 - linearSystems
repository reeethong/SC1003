float a1,b1,c1,a2,b2,c2;
   float x,y;
   printf("Enter a1,b1,c1,a2,b2,c2: \n");
   scanf("%f%f%f%f%f%f", &a1,&b1,&c1,&a2,&b2,&c2);
   if (a1*b2-a2*b1 == 0) printf("Denominator is zero!");
   else{
   x = (b2*c1-c2*b1)/(a1*b2-a2*b1);
   y = (a1*c2-c1*a2)/(a1*b2-a2*b1);
   printf("x=%.2f,", x);
   printf("y=%.2f", y);
   
}
