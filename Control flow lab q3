int i,j,n=0;
   printf("Enter the height: \n");
   scanf("%d", &n);
   printf("Pattern:\n");
   for (i=1;i<=n;i++){
       for (j=0;j<i;j++){
       if (i%3==1) printf("1");
        else if (i%3==2) printf("2");
    else printf("3");
       }
              printf("\n"); //think carefully where to put this!
}
